{
  "name": "api-service",
  "version": "1.0.0",
  "private": true,
  "main": "dist/api-service/src/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "npx tsc -p .",
    "migrate:deploy": "cd ../shared && npm run prisma:migrate:deploy",
    "migrate:dev": "cd ../shared && npm run prisma:migrate:dev",
    "db:seed": "cd ../shared && npm run prisma:seed",
    "prisma": "cd ../shared && npm run prisma:generate",
    "prestart": "npm run build && node scripts/check-and-seed.js",
    "start": "node dist/api-service/src/app.js",
    "create:restaurant": "cd ../shared && npm run prisma:generate && cd ../api-service && npx ts-node scripts/create-restaurant-enterprise.ts"
  },
  "dependencies": {
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^7.0.5",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.12",
    "qrcode": "^1.5.3",
    "resend": "^3.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "node ../shared/prisma/seed.js"
  }
}
