# مثال لملف docker-compose.override.yml
# انسخ هذا الملف إلى docker-compose.override.yml لتخصيص الإعدادات

version: '3.8'

services:
  backend:
    # مثال: تغيير المنافذ
    ports:
      - "3000:5000"  # API
      - "3001:5001"  # Socket
      - "3002:5002"  # Jobs
    
    # مثال: إضافة متغيرات بيئية إضافية
    environment:
      DEBUG: "true"
      LOG_LEVEL: "debug"
    
    # مثال: إضافة volumes للتطوير
    # volumes:
    #   - ./api-service/src:/app/api-service/src
    #   - ./socket-service/src:/app/socket-service/src
    #   - ./jobs-service/src:/app/jobs-service/src

